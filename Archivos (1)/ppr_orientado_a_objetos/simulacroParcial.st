Hora	^ precioAdicionalHora! !!Externo methodsFor: 'accessing' stamp: 'Carlos 9/19/2018 12:18'!proveedor	^ proveedor! !!Externo methodsFor: 'accessing' stamp: 'Carlos 9/19/2018 12:18'!precioAdicionalHora: anObject	precioAdicionalHora := anObject! !!Externo methodsFor: 'propios' stamp: 'Carlos 9/19/2018 12:24'!asString	|aux|	aux:= super asString .		^aux , 'Proveedor : ' ,self proveedor,	'|Precio adicional : ',self  precioAdicionalHora asString .! !!Externo methodsFor: 'propios' stamp: 'Carlos 9/19/2018 12:35'!costo	| aux |	aux:=0.	 ( super cantHoras  >0) & (self precioAdicionalHora >0)			ifTrue: [ 						aux:=  (super cantHoras * self precioAdicionalHora ) +super precioHora 				 ]			ifFalse:[ self error: 'Operacion invalida '].		 ^aux! !!Externo methodsFor: 'initialization' stamp: 'Carlos 9/19/2018 12:19'!initialize	super initialize.	precioAdicionalHora :=0.	proveedor := 'Sin cargar'.! !Hito subclass: #Interno	instanceVariableNames: 'area procentaje'	classVariableNames: ''	poolDictionaries: ''	category: 'EmpresaAdministracionSuyon'!!Interno methodsFor: 'propios' stamp: 'Carlos 9/19/2018 13:02'!asString|aux|aux:=super asString  .^ aux , 'Area : ',area ,	'|Porcentaje de actuacion  : ', self procentaje asString .	! !!Interno methodsFor: 'propios' stamp: 'Carlos 9/19/2018 13:05'!costo	| aux| 	aux:=0.	(super  costo >0)				ifTrue: [ 						aux:= (super costo )* procentaje 				 ] 			ifFalse:[					self error: 'Operacion invalida !!!!!!'				].					^aux! !!Interno methodsFor: 'accessing' stamp: 'Carlos 9/19/2018 12:36'!procentaje	^ procentaje! !!Interno methodsFor: 'accessing' stamp: 'Carlos 9/19/2018 12:36'!area	^ area! !!Interno methodsFor: 'accessing' stamp: 'Carlos 9/19/2018 12:36'!procentaje: anObject	procentaje := anObject! !!Interno methodsFor: 'accessing' stamp: 'Carlos 9/19/2018 12:36'!area: anObject	area := anObject! !!Interno methodsFor: 'initialization' stamp: 'Carlos 9/19/2