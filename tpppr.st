itialize	super initialize.	cantFiguras := nil.	cantTablas := nil.! !!Papper methodsFor: 'accessing' stamp: 'carlos 9/10/2018 20:56'!cantTablas	^ cantTablas! !!Papper methodsFor: 'accessing' stamp: 'carlos 9/10/2018 20:56'!cantTablas: anObject	cantTablas := anObject! !!Papper methodsFor: 'accessing' stamp: 'carlos 9/10/2018 20:56'!cantFiguras	^ cantFiguras! !!Papper methodsFor: 'accessing' stamp: 'carlos 9/10/2018 20:56'!cantFiguras: anObject	cantFiguras := anObject! !!Papper methodsFor: 'propios' stamp: 'carlos 9/13/2018 15:46'!obtenerValoración	"Retornar la valoración del papersegun tipo"	| valoracion |				(self informarTipo ='Full Papper')					ifTrue: [ 									valoracion :=paginas /4.						 ]					ifFalse:[								valoracion :=paginas +[(cantFiguras *cantTablas )*0.02].							].							^valoracion .! !!Papper methodsFor: 'propios' stamp: 'Carlos 9/14/2018 16:18'!validarPaginas	" La cantidad máxima de páginas que puede tener un Paper es 50 páginas"	| aux valor|	valor:=super paginas .		valor <=50		ifTrue:[				aux:=true			]		ifFalse:[				aux:=false			].		^aux ! !!Papper methodsFor: 'propios' stamp: 'carlos 9/13/2018 15:43'!calcularProporcionVisual	"retorna la suma de la cantidad de figuras y detablas y, la divide por la cantidad de páginas del paper"	| aux |	aux:= (self cantFiguras +self cantTablas)/self paginas .	^aux. ! !!Papper methodsFor: 'propios' stamp: 'Carlos 9/14/2018 16:19'!informarTipo	" retorna si el Paper es “Full	Paper” o “Short Paper”"	| aux|	aux:='Full Papper'.	(self paginas <4)		ifTrue: [aux:='Short papper.'  ].	^ aux.! !!Papper methodsFor: 'propios' stamp: 'carlos 9/13/2018 15:42'!String	"-"	 ^super String , 'Cantidad de Tablas: ',cantTablas asString ,				'Cantidad de Figuras : ',cantFiguras asString .! !Tema initialize!