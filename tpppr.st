Object subclass: #Autor	instanceVariableNames: 'nombre categoria email'	classVariableNames: ''	poolDictionaries: ''	category: 'TpiCarlosSuyon'!!Autor methodsFor: 'accessing' stamp: 'carlos 9/6/2018 16:33'!categoria	^ categoria! !!Autor methodsFor: 'accessing' stamp: 'carlos 9/6/2018 16:33'!nombre	^ nombre! !!Autor methodsFor: 'accessing' stamp: 'Carlos 9/14/2018 15:19'!nombre: anObject	"falta validar que anObject es una cadena (no numero)"		nombre := anObject 			! !!Autor methodsFor: 'accessing' stamp: 'carlos 9/13/2018 15:00'!email: anObject	"falta validar que anObject es una cadena (no numero)"	email := anObject! !!Autor methodsFor: 'accessing' stamp: 'carlos 9/13/2018 14:55'!categoria: anObject	categoria := anObject.	(categoria between: 1 and:3 )		ifTrue:[					categoria :=anObject .					]		ifFalse:[					self error: '¡Categoria invalida!!'					].					! !!Autor methodsFor: 'accessing' stamp: 'carlos 9/6/2018 16:33'!email	^ email! !!Autor methodsFor: 'propios' stamp: 'Carlos 9/12/2018 10:55'!datosDeLaClase	"comment stating purpose of message"	| aux|	aux := '			 	La descripcion es un valor entero entre 1 y 3 				1 - Docente				2-  Alumno 				3-  Investigador 			 '.		^aux! !!Autor methodsFor: 'propios' stamp: 'carlos 9/13/2018 15:14'!informarCategoria	" devuelve la descripción de la categoría de acuerdo a	su número"	| aux |		(categoria  =1 )			ifTrue: [ aux:='-Docente' ]			ifFalse:[					(categoria =2)						ifTrue: [ 									aux:='Alumno'							 ]						ifFalse:[									aux:='Investigador'							]				].			^aux! !!Autor methodsFor: 'propios' stamp: 'Carlos 9/12/2018 11:00'!String	"String de la clase "	|aux|		aux:='Nombre : ',nombre ,			'|Categora :',categoria asString ,			 ' |Dirección de email :  ',email.		^aux. ! !!Autor methodsFor: 'initialization' stamp: 'Carlos 9/14/2018 15:06'!initialize	super initialize.	categoria := 0.	email :='s/n email'.	nombre :='Desconocido'.! !Object subclass: #Congreso	instanceVariableNames: 'trabajos'	classVariableNames: ''	poolDictionaries: ''	category: 'TpiCarlosSuyon'!!Congreso methodsFor: 'initialization' stamp: 'Carlso 9/12/2018 13:10'!initialize	super initialize.	trabajos := Set new .! !!Congreso methodsFor: 'propios' stamp: 'Carlos 9/14/2018 16:40'!addTrabajo:unTrabajo	"comment stating purpose of message"	|aux valor msj|	valor:=unTrabajo titulo .	aux :=trabajos detect:[:x|x titulo = valor ]ifNone:[aux:=nil].		(aux isNotNil )		ifTrue: [ trabajos add: unTrabajo .						msj:='Se agrego el trabajo' ]		ifFalse:[				msj :='No se agrego el trabajo']			.					^aux ! !!Congreso methodsFor: 'propios' stamp: 'Carlso 9/12/2018 13:22'!String	"EL asString de la clase "	| aux |	aux:=  trabajos do: [ :x|x ].	^aux! !!Congreso methodsFor: 'propios' stamp: 'Carlos 9/14/2018 16:48'!presentoTrabajo:unAutor	"comment stating purpose of message"	| aux autorBuscado msj|		autorBuscado:=unAutor .		aux:= trabajos detect: [ :x|x autor = autorBuscado]ifNone:[  aux:=nil].		(aux isNotNil )			ifTrue: [  msj :='Se encontro el autor']			ifFalse:[msj:='No se encontro  el autor'].			^msj! !Object subclass: #Tema	instanceVariableNames: 'numero descripcion'	classVariableNames: ''	poolDictionaries: ''	category: 'TpiCarlosSuyon'!!Tema methodsFor: 'propios' stamp: 'carlos 9/13/2018 14:53'!String	"mostrar los atributos de la calse tema "	|aux|		aux:='Numero : ',numero asString ,' |Descripcion : ', descripcion asString,String cr  .	^ aux	! !!Tema methodsFor: 'propios' stamp: 'carlos 9/13/2018 14:41'!detalleTema	"mostrar las posibles descripciones "	|aux|	aux:= '1 Software y sistemas de información agropecuarios			 2 Robótica agro-industrial			 3 Redes de sensores en cultivos, tambos, feed-lots y plantas de procesamiento			 4 Monitoreo y control medio ambiental			 5 Ontologías, Big Data, Open Data y DataMining e inteligencia artificial aplicadas al agro 				 6 