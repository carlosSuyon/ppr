Object subclass: #Persona	instanceVariableNames: 'dni nombre telefono'	classVariableNames: ''	package: 'Herencia'!!Persona methodsFor: 'initialization' stamp: 'CLC 4/12/2021 10:41'!initialize	dni:=0.	nombre:=''.	telefono:=''.! !!Persona methodsFor: 'converting' stamp: 'CLC 4/12/2021 11:00'!asString	| aux |	aux:= ' Dni:', dni asString, ' Nombre: ', nombre, ' Telefono:', telefono asString.	^aux.! !!Persona methodsFor: 'accessing' stamp: 'CLC 4/12/2021 10:40'!telefono	^ telefono! !!Persona methodsFor: 'accessing' stamp: 'CLC 4/12/2021 10:40'!telefono: anObject	telefono := anObject! !!Persona methodsFor: 'accessing' stamp: 'CLC 4/12/2021 10:40'!nombre	^ nombre! ! !Persona methodsFor: 'accessing' stamp: 'CLC 4/12/2021 10:40'!nombre: anObject	nombre := anObject! !!Persona methodsFor: 'accessing' stamp: 'CLC 4/12/2021 10:40'!dni	^ dni! !!Persona methodsFor: 'accessing' stamp: 'CLC 4/12/2021 10:40'!dni: anObject	dni := anObject! !Persona subclass: #Alumno	instanceVariableNames: 'legajo promedio'	classVariableNames: ''	package: 'Herencia'!!Alumno methodsFor: 'converting' stamp: 'CLC 4/12/2021 11:05'!asString	| aux |	aux:= super asString, ' Legajo:', legajo asString, ' Promedio:', promedio asString.	^aux.! !!Alumno methodsFor: 'accessing' stamp: 'CLC 4/12/2021 10:53'!promedio: anObject	promedio := anObject! !!Alumno methodsFor: 'accessing' stamp: 'CLC 4/12/2021 10:53'!promedio	^ promedio! !!Alumno methodsFor: 'accessing' stamp: 'CLC 4/12/2021 10:53'!legajo	^ legajo! !!Alumno methodsFor: 'accessing' stamp: 'CLC 4/12/2021 10:53'!legajo: anObject	legajo := anObject! !